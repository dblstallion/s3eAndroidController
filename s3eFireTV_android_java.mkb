#!/usr/bin/env mkb
# Builder mkb file for the java portion of the s3eFireTV extension on android
platform JAVA

files
{
    (source/android)
    s3eFireTV.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"

librarypath "libs/gamecontroller.jar"

option output-name=lib/android/s3eFireTV.jar
